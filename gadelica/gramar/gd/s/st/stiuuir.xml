<module>
    <h>stiùir</h>
    <p><g>stiùir</g>
        <vb/>
        <en>steer</en></p>

    <section>
        <h>nouns</h>
        <module id="baata">
            <h>bàta</h>
            <p><g>stiùir ... <a href="?gd=baata">bàta</a></g>
                <en>... steers a boat</en> – <xl>
                    <li>
                        <gd>Stiùir Alasdair am bàta gu cala.</gd>
                        <tr>Alexander steered the boat to harbour</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                    <li>
                        <gd>Có stiùir am bàta? Stiùir Calum.</gd>
                        <tr>Who streered the boat? Calum did.</tr>
                        <ref text="617">td. 238</ref>
                    </li>
                </xl>
            </p>
        </module>
        <module id="cala">
            <h>cala</h>
            <p><g>stiùir ... gu <a href="?gd=cala">cala</a></g>
                <en>... steers to harbour</en> – <xl>
                    <li>
                        <gd>Stiùir Alasdair am bàta gu cala.</gd>
                        <tr>Alexander steered the boat to harbour</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                </xl>
            </p>
        </module>
    </section>

</module>
