<module>
    <h>sìol</h>
    <p><g>sìol</g>
        <masc/>
        <en>a seed, seed(s)</en></p>

    <section id="adjs">
        <h>adjectives</h>
        <p><g><a href="?gd=trom">trom</a> le sìol</g>
            <en>heavy with seed(s)</en> – <xl>
                <li>
                    <gd>Tha am poca trom leis an t-sìol.</gd>
                    <tr>The sack is heavy with the seed(s).</tr>
                    <ref text="617">td. 66</ref>
                </li>
            </xl>
        </p>
    </section>

    <section id="nouns">
        <h>nouns</h>
        <p><g>sìol ann am <a href="?gd=poca">poca</a></g>
            <en>seeds in a sack</en> – <xl>
                <li>
                    <gd>Tha an sìol anns a’ phoca.</gd>
                    <tr>The seed is in the sack.</tr>
                    <ref text="617">td. 66</ref>
                </li>
                <li>
                    <gd>Tha am poca trom leis an t-sìol.</gd>
                    <tr>The sack is heavy with the seed(s).</tr>
                    <ref text="617">td. 66</ref>
                </li>
            </xl>
        </p>
    </section>



</module>
