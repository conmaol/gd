<?xml version="1.0" encoding="UTF-8"?><?xml-model href="../../../module.rnc" schematypens="https://relaxng.org/ns/structure/1.0?>
<module>
    <h>seòmar</h>
    <p><g>seòmar</g>
        <masc/>
        <en>a room</en></p>

    <section id="adjs">
        <h>adjectives</h>
        <p><g>seòmar <a href="?gd=moor">mór</a></g>
            <en>a large room</en> – <xl>
                <li>
                    <gd>Tha an seòmar seo mór.</gd>
                    <tr>This room is big.</tr>
                    <ref text="617">td. 14</ref>
                </li>
            </xl></p>
        <p><g>seòmar <a href="?gd=blaath">blàth</a></g>
            <en>a warm room</en> – <xl>
                <li>
                    <gd>Tha an seòmar glè bhlàth.</gd>
                    <tr>The room is very warm.</tr>
                    <ref text="617">td. 14</ref>
                </li>
                <li>
                    <gd>A bheil an seòmar seo blàth? Chan eil.</gd>
                    <tr>Is this room warm? No, it isn’t.</tr>
                    <ref text="617">td. 14</ref>
                </li>
            </xl></p>
    </section>

    <section id="nouns">
        <h>nouns</h>
        <p><g><a href="?gd=solus">solus</a> ann an seòmar</g>
            <en>light in a room</en> – <xl>
                <li>
                    <gd>Tha solus math anns an t-seòmar seo.</gd>
                    <tr>There is good light in this room.</tr>
                    <ref text="617">td. 245</ref>
                </li>
            </xl>
        </p>
    </section>

</module>
