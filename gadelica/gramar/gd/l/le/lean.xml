<?xml version="1.0" encoding="UTF-8"?><?xml-model href="../../../module.rnc" schematypens="https://relaxng.org/ns/structure/1.0?>
<module>
    <h>lean</h>
    <p><g>lean</g>
        <vb/>
        <en>follow</en></p>

    <section>
        <h>subjects</h>
        <module id="cuu">
            <h>cù</h>
            <p><g>lean <a href="?gd=cuu">cù</a> ...</g>
                <en>a dog follows ...</en> – <xl>
                    <li>
                        <gd>Lean an cù mi do’n mhonadh.</gd>
                        <tr>The dog followed me onto the moor.</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                </xl>
            </p>
        </module>
    </section>

    <section>
        <h>objects</h>
        <module id="caar">
            <h>càr</h>
            <p><g>lean ... <a href="?gd=caar">càr</a></g>
                <en>... follows a car</en> – <xl>
                    <li>
                        <gd>Lean an càr sin!</gd>
                        <tr>Follow that car!</tr>
                        <ref text="617">td. 237</ref>
                    </li>
                    <li>
                        <gd>An do lean thu an càr? Lean.</gd>
                        <tr>Did you follow the car? Yes, I did.</tr>
                        <ref text="617">td. 237</ref>
                    </li>
                </xl>
            </p>
        </module>
    </section>

    <section>
        <h>destinations</h>
        <module id="monadh">
            <h>monadh</h>
            <p><g>lean ... do’n <a href="?gd=monadh">mhonadh</a></g>
                <en>... follows ... onto a moor</en> – <xl>
                    <li>
                        <gd>Lean an cù mi do’n mhonadh.</gd>
                        <tr>The dog followed me onto the moor.</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                </xl>
            </p>
        </module>
    </section>

</module>
