<?xml version="1.0" encoding="UTF-8"?><?xml-model href="../../../module.rnc" schematypens="https://relaxng.org/ns/structure/1.0?>
<module>
    <h>fiadh</h>
    <p><g>fiadh</g>
        <masc/>
        <en>a deer</en></p>

    <section>
        <h>verbs</h>
        <module id="ruith">
            <h>ruith</h>
            <p><g><a href="?gd=ruith">ruith</a> fiadh</g>
                <en>a deer runs</en> – <xl>
                    <li>
                        <gd>Ruith mi mar am fiadh.</gd>
                        <tr>I ran like the (a?) deer.</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                </xl>
            </p>
        </module>
        <module id="faic">
            <h>faic</h>
            <p><g><a href="?gd=faic">faic</a> ... fiadh</g>
                <en>... sees a deer</en> – <xl>
                    <li>
                        <gd>Am faca tu na féidh? Chan fhaca.</gd>
                        <tr>Did you see the deer? No, I didn’t.</tr>
                        <ref text="617">td. 177</ref>
                    </li>
                </xl>
            </p>
        </module>
    </section>

</module>
