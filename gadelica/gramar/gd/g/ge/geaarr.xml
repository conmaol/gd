<module>
    <h>geàrr</h>
    <p><g>geàrr</g>
        <vb/>
        <en>cut</en></p>

    <section>
        <h>objects</h>
        <module id="laamh">
            <h>làmh</h>
            <p><g>geàrr ... <a href="?gd=laamh">làmh</a></g>
                <en>... cuts a hand</en> – <xl>
                    <li>
                        <gd>Gheàrr Alasdair a làmh.</gd>
                        <tr>Alexander cut his hand.</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                </xl>
            </p>
        </module>
        <module id="caaise">
            <h>càise</h>
            <p><g>geàrr ... <a href="?gd=caaise">càise</a></g>
                <en>... cuts cheese</en> – 
                <xl>
                    <li>
                        <gd>Có gheàrr an càise? Gheàrr Màiri.</gd>
                        <tr>Who cut the cheese? Mary did.</tr>
                        <ref text="617">td. 47</ref>
                    </li>
                </xl>
            </p>
        </module>
    </section>

</module>
